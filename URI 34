#include <stdio.h>

int main() 
{
    int cont1 = 0, cont2 = 0;
    double vetor[6];
    scanf("%lf %lf %lf %lf %lf %lf", &vetor[0], &vetor[1], &vetor[2], &vetor[3], &vetor[4], &vetor[5]);
    
    for ( cont2 = 0; cont2 < 6; cont2++)
    {
        if (vetor[cont2] > 0.0)
        {
            cont1++;
        }
    }
    printf("%d valores positivos\n", cont1);
    
return 0;
}
