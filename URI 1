#include <stdio.h>

int main()
{
    int count, j, k;
    count = 1;
    j = 7;
    k = 0;
    
    while (count < 10) 
    {
        printf("I=%d J=%d\n", count, j);
        j -- ;
        k ++ ;
            if (k == 3)
            {
                j += 5;
                k = 0;
                count += 2;
            }
    }

return 0;
}
