#include <stdio.h>
#include <math.h>
 
int main()
{
    int A, B, C, D, s1, s2, algo; 
    scanf("%d %d %d %d", &A, &B, &C, &D);
    
    s1 = C+D;
    s2 = A+B; 
    
    if(B>C && D>A && s1 > s2 && C>0 && D>0 && A%2 == 0 )
    {
        printf("Valores aceitos\n");
    }
    else
    {
        printf("Valores nao aceitos\n");  
    }
    
    return 0;
}
